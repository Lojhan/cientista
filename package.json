    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "rollup": "^4.13.0",
    "rollup-plugin-dts": "^6.1.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "esprima": "^4.0.1"
  }
